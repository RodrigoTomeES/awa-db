{
  "name": "awa-db",
  "packageManager": "yarn@3.5.0",
  "type": "module",
  "version": "1.0.0",
  "license": "GPL-3.0+",
  "author": "Rodrigo Tom√© <hi@rodrigotome.es>",
  "scripts": {
    "dev": "astro dev",
    "start": "astro preview",
    "build": "astro build",
    "preview": "astro build && astro preview",
    "astro": "astro",
    "deploy": "gh-pages -d dist",
    "generate-favicons": "real-favicon generate favicon.config.json faviconData.json ./public",
    "prepare": "husky install",
    "lint": "eslint --fix \"**/*.{ts,tsx,js,cjs,mjs,jsx,astro}\"",
    "lint:styles": "stylelint --fix \"**/*.css\"",
    "commit": "cz",
    "update": "yarn set version stable",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@astrojs/image": "0.16.9",
    "@astrojs/prefetch": "0.2.3",
    "@astrojs/sitemap": "1.3.1",
    "@astrojs/tailwind": "3.1.3",
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-conventional": "17.6.3",
    "@ianvs/prettier-plugin-sort-imports": "4.0.0-beta.0",
    "@jasikpark/astro-svg-loader": "0.1.0",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "astro": "2.5.3",
    "astro-compress": "1.1.44",
    "astro-critters": "1.1.34",
    "cli-real-favicon": "0.0.8",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-astro": "0.27.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "gh-pages": "5.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "prettier-plugin-astro": "0.9.0",
    "prettier-plugin-tailwindcss": "0.3.0",
    "prettier-stylelint": "0.4.2",
    "sharp": "0.32.1",
    "stylelint": "15.6.2",
    "stylelint-config-idiomatic-order": "9.0.0",
    "stylelint-config-standard": "33.0.0",
    "stylelint-config-tailwindcss": "0.0.7",
    "tailwind-scrollbar-hide": "1.1.7",
    "tailwindcss": "3.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "engines": {
    "node": ">=18"
  }
}
